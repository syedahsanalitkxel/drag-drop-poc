{
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-alignment": true,
    "jsx-boolean-value": true,
    "jsx-key": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": true,
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "jsx-wrap-multiline": true,
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [true, "dev"]
  }
}
